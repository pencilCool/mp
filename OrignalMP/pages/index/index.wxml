<!--index.wxml-->

<view class="container">
  <button class="setbutton" bindtap="goSetting">设置</button>
  <view class="row">
    <!-- <view class="row">
      <button class="button">{{note}}</button>
      <button class="button" >{{scale}}</button>
      <button class="button" bindtap="tap">测试</button>
    </view> -->
    
    <view class="column">
      <view class="button" bind:touchstart="touchstartTwoOctaves" bind:touchend="touchendTwoOctaves" >高音</view>
      <view class="button" bind:touchstart="touchstartOctave" bind:touchend="touchendOctave" >中音</view>
      <view class="button" bind:touchstart="touchstartHalfStep" bind:touchend="touchendHalfStep" >#半音</view>
    </view>

    <view class="column">
      <view class="button" bind:touchstart="touchstart4" bind:touchend="touchend4" >4</view>
      <view class="button" bind:touchstart="touchstart2" bind:touchend="touchend2" >2</view>
      <view class="button" bind:touchstart="touchstart1" bind:touchend="touchend1" >1</view>
    </view>
</view>

</view>

